{
  "storage-driver": "{{ docker.daemon_options['storage-driver'] }}",
  "log-driver": "{{ docker.daemon_options['log-driver'] }}",
  "log-opts": {
    "max-size": "{{ docker.daemon_options['log-opts']['max-size'] }}",
    "max-file": "{{ docker.daemon_options['log-opts']['max-file'] }}"
  },
  "live-restore": true,
  "userland-proxy": false,
  "no-new-privileges": true,
  "default-runtime": "runc",
  "exec-opts": ["native.cgroupdriver=systemd"],
  "registry-mirrors": {{ docker.registry_mirrors | to_json }},
  "insecure-registries": [],
  "debug": false,
  "experimental": false,
  "metrics-addr": "0.0.0.0:9323",
  "iptables": true,
  "ip-forward": true,
  "ip-masq": true,
  "bridge": "docker0",
  "fixed-cidr": "",
  "default-gateway": "",
  "mtu": 0,
  "cluster-store": "",
  "cluster-advertise": "",
  "max-concurrent-downloads": 3,
  "max-concurrent-uploads": 5,
  "shutdown-timeout": 15
}